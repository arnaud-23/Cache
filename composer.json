{
    "name": "openclassrooms/cache",
    "description": "Cache library that extends Doctrine\\Cache capabilities",
    "type": "library",
    "keywords": ["cache"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Kuzniak",
            "email": "romain.kuzniak@openclassrooms.com"
        },
        {
            "name": "OpenClassrooms",
            "email": "technique@openclassrooms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenClassrooms\\": "src/OpenClassrooms/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenClassrooms\\Tests\\": "tests/OpenClassrooms/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "doctrine/cache": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "satooshi/php-coveralls": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
